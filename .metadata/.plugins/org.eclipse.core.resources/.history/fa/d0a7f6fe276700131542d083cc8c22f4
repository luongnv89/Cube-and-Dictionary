@prefix ns: <http://www.owl-ontologies.com/NguyenMai_NguyenLuong_TP.owl#>.
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.

[rule1: (?per ns:isDaughterOf ?parents)   ->  (?per rdf:type ns:Child) (?per rdf:type ns:Daughter) (?per ns:isChildOf ?parents)]
[rule2: (?per ns:isSonOf ?parents)   ->  (?per rdf:type ns:Child) (?per rdf:type ns:Son) (?per ns:isChildOf ?parents) ]
[rule3: (?per ns:isChildOf ?parents)   ->  (?parents rdf:type ns:Parent) (?parents ns:isParentOf ?per)]
[rule_mother:(?mother rdf:type ns:Female) (?mother ns:isParentOf ?child) -> (?mother rdf:type ns:Mother)(?mother ns:isMotherOf ?child)]
[rule_father:(?father rdf:type ns:Male) (?father ns:isParentOf ?child) -> (?father rdf:type ns:Father)(?father ns:isFatherOf ?child)]
[rule_sister: (?c1 ns:isDaughterOf ?p)(?c2 ns:isChildOf ?p) notEqual(?c1,?c2)-> (?c1 rdf:type ns:Sister)(?c1 ns:isSisterOf ?c2)] 
[rule_brother: (?c1 ns:isSonOf ?p)(?c2 ns:isChildOf ?p) notEqual(?c1,?c2)-> (?c1 rdf:type ns:Brother)(?c1 ns:isBrotherOf ?c2)]
[rule_sibling:(?c1 ns:isChildOf ?p)(?c2 ns:isChildOf ?p)notEqual(?c1,?c2)->(?c1 rdf:type ns:Sibling)(?c1 ns:isSiblingOf ?c2)] 
[rule_uncle: (?u ns:isBrotherOf ?c)(?c ns:isParentOf ?d) -> (?u rdf:type ns:Uncle)]
[rule_grandMother: (?gm ns:isMotherOf ?c)(?c ns:isParentOf ?d) -> (?gm rdf:type ns:GrandMother)]
[rule_grandFather: (?gf ns:isFatherOf ?c)(?c ns:isParentOf ?d) -> (?gf rdf:type ns:GrandFather)]
