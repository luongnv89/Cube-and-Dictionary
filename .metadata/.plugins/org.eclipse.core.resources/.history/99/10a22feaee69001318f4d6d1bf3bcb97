/**
 * 
 */
package mai.JUnit_Test;

import static org.junit.Assert.assertFalse;
import static org.junit.Assert.assertTrue;
import mai.process.Parallel;

import org.junit.After;
import org.junit.Test;

public class Parallel_JUnitTest extends CubeAndDic_TestSpec{


	private Parallel associatedPar;
	private Parallel notAssociatedPar;
	private Parallel invalidPar;

	/**
	 * @throws java.lang.Exception
	 */
	@After
	public void tearDown() throws Exception {
		notAssociatedPar = null;
		associatedPar = null;
		invalidPar = null;
	}


	/**
	 * Test method for {@link mai.process.Parallel#Parallel(java.lang.String, java.lang.String)}.
	 * @throws Exception 
	 */
	@Test
	public void testParallelStringString() throws Exception {
		invalidPar = new Parallel("cube_100.txt", "dicKO.txt");
		invalidPar.associatedDictionary();
	}

	/**
	 * Test method for {@link mai.process.Parallel#associatedDictionary()}.
	 * @throws InterruptedException 
	 */
	@Test
	public void testAssociatedDictionary() throws InterruptedException {
		associatedPar = new Parallel(validCube, associatedDic);
		assertTrue(associatedPar.associatedDictionary());
	}
	
	@Test
	public void testAssociatedDictionaryKO() throws InterruptedException {
		notAssociatedPar = new Parallel(validCube, notAssociatedDic);
		assertFalse(notAssociatedPar.associatedDictionary());
	}

}
